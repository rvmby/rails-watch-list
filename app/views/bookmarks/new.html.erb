<div class="container">
  <div class="row justify-content-center">
    <div class="col-12 col-md-8">
      <h1>Create your bookmark here</h1>
      <%= simple_form_for [@list, @bookmark] do |f| %>
        <%= f.association :bookmark  %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
